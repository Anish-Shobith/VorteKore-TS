{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist/",
    "removeComments": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "allowJs": true,
    "declaration": false,
    "disableSizeLimit": true
  },
  "typedocOptions": {
    "name": "discordutility",
    "readme": "README.md",
    "out": "docs"
  }
}