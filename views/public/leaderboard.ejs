<!DOCTYPE html>
<html lang="en">
<%- include ("inc/head") %>

<body class="bg-secondary">
  <%- include ("inc/nav") %>
  <section class="hero is-fullheight">
    <div class="align-middle">
      <table class="table">
        <thead>
          <tr class="has-background-primary">
            <th>Ranks</th>
            <th>User</th>
            <th>Level</th>
            <th>Xp</th>
            <th>Coins</th>
          </tr>
        </thead>
        <tbody class="has-background-light">
          <% info.forEach((member, i) => { %>
          <tr>
            <th scope="row"> <%= i + 1 %>. </th>
            <td><%= member.username %></td>
            <td><%= member.member.level %></td>
            <td><%= member.member.xp %></td>
            <td><%= member.member.coins %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </section>
</body>

</html>